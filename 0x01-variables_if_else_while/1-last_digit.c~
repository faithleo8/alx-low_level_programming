#include <stdlib.h>
#include <time.h>
#include <stdio.h>
/**
 * main - Entry point
 *
 *Return:Always 0 (success)
 */
int main(void)
{
int n;
srand(time(0));
n = rand() - RAND_MAX / 2;
last_digit = n % 10; 
if (last_digit > 0)
{ 
printf("The last digit of %d is %d", n, last_digit);
}
else if (last_digit == 0)
{
printf("The last digit of %d is %d", n, last_digit);
}
else
{
printf("The last digit of %d is %d", n, last_digit);
}
return (0);
}

